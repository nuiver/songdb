<p id="notice"><%= notice %></p>

<h1>Artists</h1>


<% @artists.each do |artist| %>
  <div class="fp_artist_box">
    <div class="fp_artist"><%= link_to artist.name, artist_songs_path(artist.id) %></div>
    <div class="fp_thumbnail"><%= image_tag(artist.image.thumbnail.url, size: "66x66") %></div>
  </div>
<% end %>



<%= link_to 'New Artist', new_artist_path %>
